                        <div id="dpDepCont"></div>

                        <div id="dpArrCont"></div>

                    </div>

                    <div class="action-bar">
                        <a id="datesDoneBtn" class="lbtn2 finish-dates-btn" href="#">@Model.W("DoneWithDatesEditingBtn")</a>
                    </div>

                </div>

