                        </div>
                        <div class="tagger-cont" id="airTagger"></div>
                    </div>

                    <div class="title">@Model.W("NewsletterFrequency")</div>
                    <div class="freq-cont">
                        <div class="itbl">

                        </div>
                    </div>

                    <div class="click-blocker" style="display: none"></div>

                </div>




            </div>
        </div>



    </script>
