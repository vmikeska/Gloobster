    <script id="custom-template" type="text/x-handlebars-template">

        <div class="form-custom">

            <div class="searches-menu">
                <a class="adder icon-plus"></a>
                <a class="show-hide" href="#"></a>
            </div>

            <div class="form-cont">

                <div class="dates-static">
                    <div class="title">@Model.W("DatesSelectionInfo")</div>
                    <div>
                        <span class="cap">@Model.W("EarliestDeparatureLatestArrival")</span>
                        <span class="earliest-dep val">02/19/2017</span>
                        <span> - </span>
                        <span class="latest-arr val">02/19/2017</span>

                        <span class="cap">@Model.W("EarliestDeparatureLatestArrival")</span>
                        <span> @Model.W("MinForDays") </span>
                        <span class="days-from val">8</span>
                        <span> @Model.W("MaxForDays") </span>
                        <span class="days-to val">12</span>
                        <span> @Model.W("DaysSentenceEnd")</span>
                    </div>
                </div>

                <div class="fixed-part">

                    <div class="title">@Model.W("HowManyDaysInDestination")</div>
                    <div class="days-range-cont"></div>

