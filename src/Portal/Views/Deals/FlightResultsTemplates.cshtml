
<script id="result-box-city-template" type="text/x-handlebars-template">
    <div class="flight-result group_{{gid}}" data-gid="{{gid}}" data-name="{{title}}">
        <div class="img-cont">

            <img class="img" src="/images/n/no-fly-img.png" />
            <div class="txt-cont">{{title}}</div>
            <div class="price-cont">
                <div class="wrap">
                    <div class="from">@Model.W("FromPrice")</div>
                    <div class="price-all">
                        <span class="currency">€</span><span class="price">{{price}}</span>
                    </div>
                </div>
            </div>

        </div>
        <div class="items">

            <table></table>

        </div>
    </div>
</script>

<script id="result-box-city-small-template" type="text/x-handlebars-template">
    <div class="flight-result-small" data-gid="{{gid}}" data-name="{{title}}">

        <div class="cell l">
            <span class="tit">{{title}}</span>
        </div>
        <div class="cell m">


            <div class="price-box">
                <div class="wrap">
                    <div class="from">@Model.W("FromPrice")</div>

                    <div class="price-all">
                        <span class="currency">€</span><span class="price">{{price}}</span>
                    </div>
                </div>
            </div>


        </div>
        <div class="cell r">
            <table></table>
        </div>

    </div>
</script>


<script id="result-box-country-template" type="text/x-handlebars-template">
    <div class="flight-result group_{{cc}}" data-cc="{{cc}}">
        <div class="img-cont">

            <div class="img-wrap">
                @*<img src="/images/n/flags-square/{{cc}}.svg" />*@
                <img class="img" src="/images/n/no-fly-img.png" />
            </div>

            <div class="txt-cont darker">{{title}}</div>
            <div class="price-cont">
                <div class="wrap">
                    <span class="from">@Model.W("FromPrice")</span>
                    <br />
                    <span class="price-all">
                        <span class="currency">€</span><span class="price">{{price}}</span>
                    </span>
                </div>
            </div>

        </div>
        <div class="items">

            <table></table>

        </div>
    </div>
</script>


