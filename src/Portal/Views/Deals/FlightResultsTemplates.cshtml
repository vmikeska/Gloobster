<script id="one-offer-airports-template" type="text/x-handlebars-template">
    <tr class="one-city-offer" data-f="{{from}}" data-t="{{to}}">
        <td><span class="bold">{{from}} → {{to}}</span></td>
        <td>@Model.W("FromPrice")</td>
        <td><span class="bold">€{{price}}</span></td>
        <td><a class="btn" href="#">@Model.W("SeeOffersBtn")</a></td>
    </tr>
</script>

<script id="one-offer-city-template" type="text/x-handlebars-template">
    <tr data-gid="{{gid}}">
        <td><span class="bold">{{name}}</span></td>
        <td>@Model.W("FromPrice")</td>
        <td><span class="bold">€{{price}}</span></td>
        <td><a class="btn" href="#">@Model.W("SeeOffersBtn")</a></td>
    </tr>
</script>

<script id="offers-expander-template" type="text/x-handlebars-template">
    <tr class="expander">
        <td colspan="4">
            <a class="btn" href="#">@Model.W("MoreBtn")</a>
        </td>
    </tr>
</script>

<script id="offers-collapser-template" type="text/x-handlebars-template">
    <tr class="collapser">
        <td colspan="4">
            <a class="btn" href="#">@Model.W("LessBtn")</a>
        </td>
    </tr>
</script>






<script id="weekend-week-template" type="text/x-handlebars-template">
    <div class="weekend-item" data-w="{{week}}" data-t="{{year}}">

        <div class="title">
            <span class="week">{{week}}. @Model.W("WeekendNo")</span> - <span class="date">{{dateFrom}} - {{dateTo}}</span>
        </div>

        <div class="cont"></div>
    </div>

</script>


