

<script id="status-template" type="text/x-handlebars-template">

    <div>
        <b>Your status</b> waiting in: {{placeName}}, want meet: {{wantMeetName}}, wantDo: {{wantDoName}}
        <a href="#">Edit</a>
    </div>

</script>

<script id="checkinDialog-template" type="text/x-handlebars-template">
    <div class="popup" id="checkin-dialog">
        <div>
            <h3>Checkin dialog</h3>
            <div class="checkinCont">

                <div>
                    <span>Want do</span>
                    <div id="wantDoCont">

                    </div>                    
                </div>

                <div>
                    <span>Want meet</span>
                    <input id="wantMeetMan" type="radio" name="wantMeet" value="0"><label for="wantMeetMan">Man only</label>
                    <input id="wantMeetWoman" type="radio" name="wantMeet" value="1"><label for="wantMeetWoman">Woman only</label>
                    <input id="wantMeetAnybody" type="radio" name="wantMeet" value="2"><label for="wantMeetAnybody">Doesn't matter</label>
                    <br />
                    <input id="multiPeopleAllowed" type="checkbox" />
                    <label for="multiPeopleAllowed">Multiple people allowed</label>
                </div>

                <div>
                    <span>AgeRange</span>
                    <div id="fromAge" class="dropdown">
                        <span class="selected">18</span>
                        <ul>                            
                        </ul>
                        <input type="hidden" value="18">
                    </div>

                    <div id="toAge" class="dropdown">
                        <span class="selected">35</span>
                        <ul>                            
                        </ul>
                        <input type="hidden" value="35">
                    </div>

                </div>
                
                <div>
                    <span>Checkin message</span>
                    <br />
                    <textarea id="chckMsg"></textarea>
                </div>

                <div id="dlgSpec">

                </div>
               

            </div>
            <div class="tright">
                <a class="button border cancel" href="#">Cancel</a>
                <a id="submitCheckin" class="button confirm" href="#">{{sumbitText}}</a>
            </div>
            <a class="close" href="#"></a>
        </div>
    </div>
</script>

<script id="dlgNow-template" type="text/x-handlebars-template">
    <span>Place</span>
    <br />
    <div id="waitingPlace" class="dropdown">
        <input class="inputed" type="text" placeholder="Search for place name">
        <img class="loader" src="/images/loader-gray.gif" style="display: none" />
        <ul></ul>
    </div>

    <br />
    <br />

    <span>Waiting time</span>
    <br />
    <input id="minsWaiting" type="text" value="60" />

</script>

