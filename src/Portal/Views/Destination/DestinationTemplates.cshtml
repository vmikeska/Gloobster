<script id="connection-single-flight-template" type="text/x-handlebars-template">

        <div class="flight-bck-cont {{customClass}}">

            <div class="flight">
                <div class="top-line">
                    {{airName}}
                </div>

                <div class="data-tbl">

                    <div class="di logo-cont">
                        <img class="air-logo" src="{{logoLink}}" />
                    </div>

                    <div class="di flight-dir">
                        <div class="from-to">{{codeFrom}} → {{codeTo}}</div>
                        <div class="flight-no">{{flightNo}}</div>
                    </div>

                    <div class="di">
                        <div class="date">{{date}}</div>
                    </div>

                    <div class="di time-cont">
                        <div class="time">{{timeFrom}} - {{timeTo}}</div>
                        <div class="duration">({{durHours}}h {{durMins}}min)</div>
                    </div>

                    <div class="di direct-cont">

                        {{#ifCond stops 0}}
                        <div class="direct">Direct</div>
                        {{else}}
                        <div class="stops">
                            <a class="stops-btn" href="#"><span class="txt">{{stops}} Stops </span><span class="icon-dropdown-arrow"></span></a>
                        </div>
                        {{/ifCond}}

                    </div>

                </div>

            </div>
        </div>

    </script>

    <script id="connection-flight-template" type="text/x-handlebars-template">

        <div class="conn">

            <div class="layout-tbl">
                <div class="left">

                    @*<div class="service">
                            <a href="#"><span class="txt">show detail </span><span class="icon-dropdown-arrow"></span></a>
                        </div>*@

                </div>
                <div class="right">

                    <div class="book-zone">
                        <div class="score">
                            <div class="txt-score">
                                {{#ifCond scoreText "E"}}Excellent{{/ifCond}}
                                {{#ifCond scoreText "G"}}Good{{/ifCond}}
                                {{#ifCond scoreText "S"}}Standard{{/ifCond}}
                            </div>
                            <div class="stars-score">

                                {{#for 0 scoreStars 1}}
                                <span class="icon-star-full"></span>
                                {{/for}}

                            </div>
                            <div class="cap">
                                Flight score
                            </div>
                        </div>
                        <div class="price-all">
                            <span class="currency">€</span><span class="price">{{price}}</span>
                        </div>

                        <div class="btn-cont">
                            <a href="#">Book now</a>
                        </div>
                    </div>



                </div>
            </div>

        </div>

    </script>



