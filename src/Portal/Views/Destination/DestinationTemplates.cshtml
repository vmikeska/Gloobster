    <script id="custom-template" type="text/x-handlebars-template">

        <div class="blue-form margin form">
            <div class="table future-table">
                <div class="cell">
                    <span class="future-label"></span>
                </div>
                <div class="cell">
                    <button id="addNewItem">[+]</button>
                    <input id="nameInput" style="display: none" />

                    <button id="nameSaveBtn" style="display: none">[s]</button>
                    <div class="dropdown future-select-full">

                        <span id="selectedSpan" class="selected"></span>
                        <button id="nameEditBtn">[e]</button>
                        <ul id="searchesList"></ul>
                        <input type="hidden">
                    </div>
                </div>
            </div>
            <div class="table future-table">
                <div class="cell">
                    <span class="future-label">From</span>
                </div>
                <div class="cell">
                    <div id="fromAirportsTagger" class="tags future-tags-full">

                        @*<a class="tag" href="#">Frankfurt &bull; FRA</a>
                            <a class="tag" href="#">Barcelona &bull; BCN</a>

                            <div class="dropdown dropdown-open">
                                    <input class="inputed" placeholder="+ Add Tag">
                                    <ul>
                                            <li>Dropdown Menu Item</li>
                                            <li>Another Item</li>
                                            <li>The Last One</li>
                                    </ul>
                            </div>*@

                        <a class="close" href="#">Close</a>
                    </div>
                </div>
            </div>
            <div class="table future-table">
                <div class="cell">
                    <span class="future-label">Time</span>
                </div>
                <div class="cell">
                    <div id="timeTagger" class="tags future-tags-full">
                        <a class="close" href="#">Close</a>
                    </div>
                </div>
            </div>
            <div id="lengthRadio" class="future-line">
                <span class="color1">Rough duration</span>
                <input id="radio1" data-vl="0" name="radio" type="radio" checked>
                <label class="mleft10" for="radio1">Not important</label>
                <input id="radio2" data-vl="3" name="radio" type="radio">
                <label class="mleft10" for="radio2">Weekend</label>
                <input id="radio3" data-vl="7" name="radio" type="radio">
                <label class="mleft10" for="radio3">Week</label>
                <input id="radio4" data-vl="14" name="radio" type="radio">
                <label class="mleft10" for="radio4">Two weeks</label>
                <input id="radio5" data-vl="custom" name="radio" type="radio">
                <label class="mleft10" for="radio5">Custom</label>
                <input style="display: none" id="customLength" type="text" value="0" />
            </div>
        </div>

    </script>
