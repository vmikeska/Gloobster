

<script id="hint-template" type="text/x-handlebars-template">
    
    <div class="info-dlg">
        <div class="wrapper">
            <span class="icon-bell"></span>
            <a class="icon-close-cross close" href="#"></a>

            <div class="cont">{{message}}</div>
        </div>
    </div>
    
</script>




<script id="calendar-base-template" type="text/x-handlebars-template">
    <div class="my-calendar-win">

        <div class="months">
            <div class="l-arr"><span class="prev move-month">&#60;</span></div>
            <div class="date"></div>
            <div class="r-arr"><span class="next move-month">&#62;</span></div>
        </div>
        <div class="days-title">            
        </div>
        <div class="bottom">
            <span class="icon-cross-small close-cal"></span>
        </div>
    </div>
</script>


<script id="placeItem-template" type="text/x-handlebars-template">
        <li data-value="{{sourceId}}" data-type="{{sourceType}}"><span class="{{icoClass}} ico"></span> <span class="txt">{{name}}</span>, <span class="cc">{{countryCode}}</span></li>
    </script>

    <script id="userItem-template" type="text/x-handlebars-template">
        <li data-value="{{userId}}"><span class="thumb"><img src="{{photoUrl}}"></span><span class="txt">{{displayName}}</span></li>
    </script>

<script id="confirmDialog-template" type="text/x-handlebars-template">
        <div class="popup" id="popup-confirm">
            <div>
                <h3>{{title}}</h3>
                <p>{{text}}</p>                
                <div class="btns2">
                    <a class="lbtn2 cancel-btn cancel" href="#">{{textCancel}}</a>
                    <a class="lbtn2 confirm-btn confirm" href="#">{{textOk}}</a>
                </div>
                <a class="icon-close-cross close" href="#"></a>
            </div>
        </div>
    </script>

<script id="error-dialog-template" type="text/x-handlebars-template">
    <div class="popup popup-error" id="popup-error" style="display: block">
        <div>
            <span class="first">
                OOPS :(
            </span>
            <span class="second">
                @Model.W("GeneralErrorMessageText", "layout")
            </span>
            <span class="third">
                <a class="lbtn2 refresh-btn" href="#">@Model.W("GeneralErrorMessageRefreshBtn", "layout")</a>
            </span>
            <span class="error">
                {{error}}
            </span>
        </div>
    </div>
</script>

<script id="infoDialog-template" type="text/x-handlebars-template">
        <div class="popup" id="popup-info">
            <div>
                <h3>{{title}}</h3>
                <p>{{text}}</p>
                <div class="btns2">
                    <a class="lbtn2 ok-btn cancel" href="#">@Model.W("Close", "layout")</a>
                </div>
                <a class="close" href="#"></a>
            </div>
        </div>
    </script>

    <script id="inPorgressDialog-template" type="text/x-handlebars-template">

        <div class="popup-n inprogress wrapper">
            <div class="top-line"></div>
            <div class="cont">
                <img src="/images/loader-gray.gif" />
                <span>{{message}}</span>
            </div>
        </div>

    </script>

    <script id="tagger-template" type="text/x-handlebars-template">
        <div class="dropdown tagger">
            <input class="inputed" type="text" placeholder="{{placeholder}}" />
            <span class="icon-search"></span>
            <ul style="display: none"></ul>
        </div>
    </script>
